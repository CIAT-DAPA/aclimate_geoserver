<?xml version="1.0" encoding="UTF-8"?>
<StyledLayerDescriptor xmlns="http://www.opengis.net/sld" version="1.0.0" xmlns:ogc="http://www.opengis.net/ogc" xmlns:sld="http://www.opengis.net/sld" xmlns:gml="http://www.opengis.net/gml">
  <UserLayer>
    <sld:LayerFeatureConstraints>
      <sld:FeatureTypeConstraint/>
    </sld:LayerFeatureConstraints>
    <sld:UserStyle>
      <sld:Name>seasonal_below</sld:Name>
      <sld:Abstract>seasonal preciptiation forecast probablistic below</sld:Abstract>
      <sld:FeatureTypeStyle>
        <sld:Rule>
          <sld:RasterSymbolizer>
            <sld:ChannelSelection>
              <sld:GrayChannel>
                <sld:SourceChannelName>1</sld:SourceChannelName>
              </sld:GrayChannel>
            </sld:ChannelSelection>
            <sld:ColorMap type="ramp">
              <sld:ColorMapEntry color="#ffffff" label="0" quantity="0" opacity="0"/>
              
              <sld:ColorMapEntry quantity="30.0" label="≤30"     color="#fef0d9"/>
              <sld:ColorMapEntry quantity="35.0" label="30 - 35" color="#fef0d9"/>
              <sld:ColorMapEntry quantity="40.0" label="35 - 40" color="#fddbac"/>
              <sld:ColorMapEntry quantity="45.0" label="40 - 45" color="#fdc383"/>
              <sld:ColorMapEntry quantity="50.0" label="45 - 50" color="#fc9f67"/>
              <sld:ColorMapEntry quantity="55.0" label="50 - 55" color="#f57a4e"/>
              <sld:ColorMapEntry quantity="60.0" label="55 - 60" color="#e75438"/>
              <sld:ColorMapEntry quantity="65.0" label="60 - 65" color="#ce2a1d"/>
              <sld:ColorMapEntry quantity="70.0" label="65 - 70" color="#b30000"/>
              <sld:ColorMapEntry quantity="71.0" label="≥70"   	 color="#b30000"/>
              
              
            </sld:ColorMap>
          </sld:RasterSymbolizer>
        </sld:Rule>
      </sld:FeatureTypeStyle>
    </sld:UserStyle>
  </UserLayer>
</StyledLayerDescriptor>
